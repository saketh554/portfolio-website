{
  "extends": ["airbnb", "prettier"],
  "plugins": ["prettier"],
  "env": {
    "browser": true,
    "node": true,
    "es2021": true
  },
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
  "react/no-unescaped-entities": "off",
  "react/prop-types": "off",
  "camelcase": "off",
  "jsx-a11y/label-has-associated-control": "off",
  "react/button-has-type": "off",
  "react/no-array-index-key": "off",
  "consistent-return": "off",
  "react/no-danger": "off",
  "no-console": "off"
}

}
